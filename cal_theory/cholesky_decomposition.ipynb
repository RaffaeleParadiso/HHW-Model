   "source": [
    "from IPython.display import display\n",
    "from sympy import *\n",
    "x, y, rho_sv, rho_sr, rho_vr = symbols(r'x y rho_sv, rho_sr, rho_vr')\n",
    "\n",
    "M = Matrix([\n",
    "    [1, rho_sv, rho_sr ],\n",
    "    [ rho_sv, 1, rho_vr],\n",
    "    [rho_sr, rho_vr, 1 ]\n",
    "])\n",
    "\n",
    "print(M == M.T)\n",
    "L = M.cholesky(hermitian=False)\n",
    "display(L)"
   ]
