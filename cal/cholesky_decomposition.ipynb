   "source": [
    "from IPython.display import display\n",
    "from sympy import *\n",
    "x, y, r_sv, r_sr, r_vr = symbols('x y r_sv, r_sr, r_vr')\n",
    "\n",
    "M = Matrix([\n",
    "    [1, r_sv, r_sr ],\n",
    "    [ r_sv, 1, r_vr],\n",
    "    [r_sr, r_vr, 1 ]\n",
    "])\n",
    "\n",
    "print(M == M.T)\n",
    "L = M.cholesky(hermitian=False)\n",
    "display(L)"
   ]
